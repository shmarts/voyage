{
  "manifest_version": 3,
  "name": "voyage",
  "version": "1.0.0",
  "description": "voyage extension",
  "permissions": ["tabs"],
  "host_permissions": ["*://*/*"],
  "action": {
    "default_title": "Vue Popup"
  },
  "background": {
    "service_worker": "background.ts"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["main.ts"]
    }
  ]
}
